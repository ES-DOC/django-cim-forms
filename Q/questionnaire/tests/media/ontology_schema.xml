<?xml version="1.0" encoding="UTF-8"?>
<ontology>
    <name>test ontology</name>
    <version>2.0</version>
    <description>This is a pretend CIM version used for testing purposes only. If you are seeing
        this in the production site, then something has gone horribly wrong.</description>
    <classes>
        <class package="science" stereotype="document">
            <name>model</name>
            <description/>
            <attributes>
                <attribute is_label="true" is_nillable="true">
                    <name>name</name>
                    <description/>
                    <cardinality min="1" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>                
                <attribute is_nillable="true">
                    <name>enumeration</name>
                    <description>A test enumeration.</description>
                    <cardinality min="0" max="1"/>
                    <type>ENUMERATION</type>
                    <enumeration is_open="true">
                        <choices>
                            <choice>
                                <value>one</value>
                                <description>documentation for one.</description>
                            </choice>
                            <choice>
                                <value>two</value>
                                <description>documentation for two.</description>
                            </choice>
                            <choice>
                                <value>three</value>
                                <description>documentation for three.</description>
                            </choice>
                        </choices>
                    </enumeration>
                </attribute>
                <attribute is_nillable="true">
                    <name>thing</name>
                    <description>A relationship property;          
                    there are lots of spaces in this documentation.</description>
                    <cardinality min="0" max="1"/>
                    <type>RELATIONSHIP</type>
                    <relationship is_recursive="true">
                        <targets>
                            <target>shared.recursive_thing</target>
                        </targets>
                    </relationship>
                </attribute>
            </attributes>
        </class>
        <class package="shared">
            <name>recursive_thing</name>
            <description/>
            <attributes>
                <attribute is_nillable="true">
                    <name>name</name>
                    <cardinality min="1" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>
                <attribute is_nillable="true">
                    <name>child</name>
                    <cardinality min="0" max="N"/>
                    <type>RELATIONSHIP</type>
                    <relationship is_recursive="true">
                        <targets>
                            <target>shared.recursive_thing</target>
                        </targets>
                    </relationship>
                </attribute>
                <attribute is_nillable="true">
                    <name>multiple_targets</name>
                    <cardinality min="0" max="1"/>
                    <type>RELATIONSHIP</type>
                    <relationship>
                        <targets>
                            <target>shared.other_thing_one</target>
                            <target>shared.other_thing_two</target>
                        </targets>
                    </relationship>
                </attribute>
            </attributes>
        </class>
        <class package="shared">
            <name>other_thing_one</name>
            <description/>
            <attributes>
                <attribute is_nillable="true">
                    <name>name</name>
                    <cardinality min="1" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>                
            </attributes>
        </class>
        <class package="shared">
            <name>other_thing_two</name>
            <description/>
            <attributes>
                <attribute is_nillable="true">
                    <name>name</name>
                    <cardinality min="1" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>                
            </attributes>
        </class>
    </classes>
</ontology>
