<?xml version="1.0" encoding="UTF-8"?>
<ontology>
    <name>test</name>
    <version>2.0</version>
    <description>This is a pretend CIM version used for testing purposes only. If you are seeing
        this in the production site, then something has gone horribly wrong.</description>
    <classes>
   
        <class package="science" stereotype="document">
            <name>modelComponent</name>
            <description>A ModelCompnent is nice.</description>
            <attributes>
                <attribute is_label="true">
                    <name>string</name>
                    <description>I am a string</description>
                    <cardinality min="1" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>
                <attribute>
                    <name>boolean</name>
                    <description>I am a boolean</description>
                    <cardinality min="0" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>BOOLEAN</atomic_type>
                    </atomic>
                </attribute>
                <attribute>
                    <name>date</name>
                    <description>I am a date</description>
                    <cardinality min="0" max="1"/>
                    <type>ATOMIC</type>                    
                    <atomic>
                        <atomic_type>DATE</atomic_type>
                    </atomic>
                </attribute>
                <attribute>
                    <name>uncategorized</name>
                    <description>I am an uncategorized string</description>
                    <cardinality min="0" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>
                <attribute>
                    <name>enumeration</name>
                    <description>I am an enumreation</description>
                    <cardinality min="1" max="N"/>
                    <type>ENUMERATION</type>
                    <enumeration is_multi="false" is_nillable="false" is_open="false">
                        <choices>
                            <choice>
                                <value>one</value>
                                <description>documentation for choice one</description>
                            </choice>
                            <choice>
                                <value>two</value>
                            </choice>
                            <choice>
                                <value>three</value>
                            </choice>
                        </choices>
                    </enumeration>
                </attribute>
                <attribute>
                    <name>author</name>
                    <description>I am a relationship</description>
                    <cardinality min="0" max="1"/>
                    <type>RELATIONSHIP</type>
                    <relationship>
                        <targets>
                            <target>shared.responsibleParty</target>
                        </targets>
                    </relationship>
                </attribute>
                <attribute>
                    <name>contact</name>
                    <description>I am a relationship</description>
                    <cardinality min="0" max="N"/>
                    <type>RELATIONSHIP</type>
                    <relationship>
                        <targets>
                            <target>shared.responsibleParty</target>
                        </targets>
                    </relationship>
                </attribute>
            </attributes>
        </class>

        <class package="shared">
            <name>responsibleParty</name>
            <description>a stripped-down responsible party to use for testing
                purposes.</description>
            <attributes>
                <attribute package="shared" is_label="true">
                    <name>individualName</name>
                    <description/>
                    <cardinality min="1" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>
                <attribute package="shared">
                    <name>contactInfo</name>
                    <description/>
                    <cardinality min="1" max="1"/>
                    <type>RELATIONSHIP</type>
                    <relationship>
                        <targets>
                            <target>shared.contactType</target>
                        </targets>
                    </relationship>
                </attribute>
            </attributes>
        </class>

        <class package="shared">
            <name>contactType</name>
            <description>a stripped-down contactType just for testing purposes.</description>
            <attributes>
                <attribute package="shared">
                    <name>phone</name>
                    <description/>
                    <cardinality min="0" max="1"/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>
                <attribute package="shared">
                    <name>address</name>
                    <description/>
                    <cardinality min="0" max="1"/>
                    <type>ATOMIC</type>                    
                    <atomic>
                        <atomic_type>TEXT</atomic_type>
                    </atomic>
                </attribute>
            </attributes>
        </class>

    </classes>
</ontology>
