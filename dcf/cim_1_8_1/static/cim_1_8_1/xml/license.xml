{# this is the xml template for license (v1.8.1) #}

{% load dcf_filters %}

{% block content %}

    <license unrestricted={{model.unrestricted}}>

        {% if model.licenseName %}
        <licenseName>{{model.licenseName}}</licenseName>
        {% endif %} {# /model.licenseName #}

        {% if model.licenseContact %}
        <licenseContact>{{model.licenseContact}}</licenseContact>
        {% endif %} {# /model.licenseContact #}

        {% if model.licenseDescription %}
        <licenseDescription>{{model.licenseDescription}}</licenseDescription>
        {% endif %} {# /model.licenseDescription #}
     
    </license>
    
{% endblock %} {# /content #}


