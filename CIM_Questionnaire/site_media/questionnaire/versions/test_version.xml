                        <target>parallelisation</target>
                    </relationship>
                </attribute>                
                <attribute>
                    <name>platform</name>
                    <description>The platform that this deployment has been run on.  It is optional to allow for "unconfigured" models, that nonetheless specify their parallelisation constraints (a feature needed by OASIS).</description>
                    <type>RELATIONSHIP</type>
                    <relationship>
                        <cardinality min="0" max="1"/>
                        <target>platform</target>
                    </relationship>
                </attribute>                
                <attribute>
                    <name>executableName</name>
                    <description/>
                    <type>ATOMIC</type>
                    <atomic>
                        <atomic_type>STRING</atomic_type>
                    </atomic>
                </attribute>                
                <attribute>
