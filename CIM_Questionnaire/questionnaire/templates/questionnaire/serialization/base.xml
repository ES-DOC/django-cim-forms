<?xml version="1.0" encoding="UTF-8"?>

{% load serialization_filters %}

{% spaceless %}
    <{{ proxy.name }}
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:gml="http://www.opengis.net/gml/3.2"
        xmlns:xlink="http://www.w3.org/1999/xlink"
        xmlns:gco="http://www.isotc211.org/2005/gco"
        xmlns:gmd="http://www.isotc211.org/2005/gmd"
        xmlns="{{ version.url|get_url_path }}" xsi:schemaLocation="{{ version.url }}"
        {% for property in model|get_standard_properties_with_stereotypes:"attribute" %}
            {{ property.name }}="{{ property.atomic_value }}"
        {% endfor %}
    >

        <!-- document generated by CIM Questionnaire (v{{questionnaire_version}}) at {% now "c" %} -->
        <!-- local pk = {{ model.pk }} -->

        {% block content %}
            you forgot to provide content!
        {% endblock %} {# /content #}

    </{{ proxy.name }}>
{% endspaceless %}


